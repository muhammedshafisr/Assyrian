	<script>
		function addToCart(productId){
			$.ajax({
				url : '/addToCart',
				data : {
					productId,
					count : 1
				},
				method : 'post',
				success : (response) => {
					if(response.status == 'itemAddedToTheCart' || response.status == 'redirect'){
						window.location.href = '/shoppingCart'
					}
				}
			})
		}
	</script>

