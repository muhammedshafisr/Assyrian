		<div class="cart-main-area main-container">
			<div class="entry-header">
				<h1 class="entry-title">Wishlist</h1>
			</div>
			<div class="container">
				<div class="row">
                    <%var items%>
                    <% if (items) {%>
					<div class="table-content table-responsive">
                        <table>
                            <thead>
                                <tr>
                                    <th class="product-thumbnail">Images</th>
                                    <th class="product-name">Product</th>
                                    <th class="product-price">Price</th>
                                    <th class="product-quantity">Stock Status</th>
                                    <th class="product-subtotal">Add to cart</th>
                                    <th class="product-remove">Remove</th>
                                </tr>
                            </thead>
                            
                            <tbody>
								<%items.forEach((x) => {%>
                                <tr>
                                    <td class="product-thumbnail"><a href="#"><img src="img/product/<%= x.result[0].imageOne%>" alt="" /></a></td>
                                    <td class="product-name"><a href="#"><%= x.result[0].productName%></a></td>
                                    <td class="product-price"><span class="amount" id="hlo">â‚¹<%= x.result[0].price%></span></td>
                                    <td class="product-quantity"><%= x.result[0].status%></td>
									<%if(x.result[0].status == 'out of stock'){%>
										<td class="product-subtotal"><p><a style="align-self: center;" class="btn disabled" role="button" aria-disabled="true">Add to cart</a></p></td>
									<%}else{%>
                                    <td class="product-subtotal"><p><a style="align-self: center;" onclick="addToCart('<%=x.result[0]._id%>')" class="btn btn-outline-info">Add to cart</a></p></td>
									<%}%>
                                    <td class="product-remove"><a href="removeWishlistItem?id=<%=x.result[0]._id%>"><i class="fa fa-times"></i></a></td>
                                </tr>
								<%})%>
                            </tbody>
                         
                        </table>
                    </div>
                    <%}else {%>
						<div class="container d-flex justify-content-center" style="min-height: 30vh;">
							<div class="row align-items-center">

								<a class="btn btn-outline-primary" href="/">Continue Shopping</a>

							</div>
						</div>
                    <%}%>
				</div>
			</div>
		</div>
