    <script>

        const inpOfName = document.getElementById('inpOfName')
        const inpOfPhone = document.getElementById('inpOfPhone')
        const inpOfMail = document.getElementById('inpOfMail')
        const button = document.getElementById('button')
        
        

        document.getElementById('userName').addEventListener('click', (event) => {
            button.disabled = false
            if (inpOfName.disabled) {
                inpOfName.disabled = false
            } else {
                inpOfName.disabled = true
            }

        })

        document.getElementById('userPhone').addEventListener('click', (event) => {
            button.disabled = false
            if (inpOfPhone.disabled) {
                inpOfPhone.disabled = false
            } else {
                inpOfPhone.disabled = true
            }

        })

        document.getElementById('userEmail').addEventListener('click', (event) => {
            button.disabled = false
            if (inpOfMail.disabled) {
                inpOfMail.disabled = false
            } else {
                inpOfMail.disabled = true
            }

        })

        document.getElementById('imglink').addEventListener('click', (event) => {
            button.disabled = false
        })
    </script>

