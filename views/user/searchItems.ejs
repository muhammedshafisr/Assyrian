<body>


    <%- include('../partials/userNav.ejs')%>

        <section style="background-color: #eee; min-height: 100%;">
            <% if (!resolve.length){%>
                <h6 style="text-align: center;">No matching result for <%= text%></h6>
                <%}else{%>
                    <h6 style="text-align: center;">Search result for <%= text%></h6>
            <div class="container align-items-center">
                <%resolve.forEach((x)=> {%>
                    <div class="row justify-content-center mb-3">
                        <div class="col-md-12 col-xl-10">
                            <div class="card shadow-0 border rounded-3">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-md-12 col-lg-3 col-xl-3 mb-4 mb-lg-0">
                                            <div class="bg-image hover-zoom ripple rounded ripple-surface">
                                                <img src="/img/product/<%= x.imageOne%>" class="w-100" />
                                                <a href="/viewProduct<%= x._id%>">
                                                    <div class="hover-overlay">
                                                        <div class="mask"
                                                            style="background-color: rgba(253, 253, 253, 0.15);">
                                                        </div>
                                                    </div>
                                                </a>
                                            </div>
                                        </div>
                                        <div class="col-md-6 col-lg-6 col-xl-6">
                                            <h5>
                                                <%= x.productName%>
                                            </h5>
                                            <h6>
                                                <%= x.description%>
                                            </h6>
                                            <div class="d-flex flex-row">
                                                <div class="text-danger mb-1 me-2">
                                                    <i class="fa fa-star"></i>
                                                    <i class="fa fa-star"></i>
                                                    <i class="fa fa-star"></i>
                                                    <i class="fa fa-star"></i>
                                                </div>
                                                <span>310</span>
                                            </div>
                                            <div class="mt-1 mb-0 text-muted small">
                                                <span>Fabric</span>
                                                <span class="text-primary"> • </span>
                                                <span>
                                                    <%= x.fabric%>
                                                </span>
                                                <!-- <span class="text-primary"> • </span>
                                        <span>Best finish<br /></span> -->
                                            </div>
                                            <div class="mb-2 text-muted small">
                                                <span>Sleeve</span>
                                                <span class="text-primary"> • </span>
                                                <span>
                                                    <%= x.sleeve%>
                                                </span>
                                                <!-- <span class="text-primary"> • </span>
                                        <span>Casual<br /></span> -->
                                            </div>
                                            <div class="mb-2 text-muted small">
                                                <span>Pack Of</span>
                                                <span class="text-primary"> • </span>
                                                <span>
                                                    <%= x.packOf%>
                                                </span>
                                            </div>
                                            <!-- <p class="text-truncate mb-4 mb-md-0">
                                        There are many variations of passages of Lorem Ipsum available, but the
                                        majority have suffered alteration in some form, by injected humour, or
                                        randomised words which don't look even slightly believable.
                                    </p> -->
                                        </div>
                                        <div class="col-md-6 col-lg-3 col-xl-3 border-sm-start-none border-start">
                                            <div class="d-flex flex-row align-items-center mb-1">
                                                <h4 class="mb-1 me-1">₹<%=x.price%>
                                                </h4>
                                                <span class="text-danger"><s>₹<%= x.discount%></s></span>
                                            </div>
                                            <h6 class="text-success">Free shipping</h6>
                                            <div class="d-flex flex-column mt-4">
                                                <a class="btn btn-outline-primary btn-sm mt-2" type="button" href="/viewProduct<%= x._id%>">View Product</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <%})%>
            </div>
            <%}%>
        </section>







