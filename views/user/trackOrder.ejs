    <style>
        .track-line {
            height: 2px !important;
            background-color: #488978;
            opacity: 1;
        }

        .dot {
            height: 10px;
            width: 10px;
            margin-left: 3px;
            margin-right: 3px;
            margin-top: 0px;
            background-color: #488978;
            border-radius: 50%;
            display: inline-block
        }

        .big-dot {
            height: 25px;
            width: 25px;
            margin-left: 0px;
            margin-right: 0px;
            margin-top: 0px;
            background-color: #488978;
            border-radius: 50%;
            display: inline-block;
        }

        .big-dot i {
            font-size: 12px;
        }

        .card-stepper {
            z-index: 0
        }
    </style>
</head>

<body>

    <%- include('../partials/userNav.ejs')%>

        <section class="h-100" style="background-color: #eee;">
            <div class="container py-5 h-100">
                <div class="row d-flex justify-content-center align-items-center h-75">
                    <div class="col">
                        <div class="card card-stepper" style="border-radius: 10px;">
                            <div class="card-body p-4">

                                <div class="d-flex justify-content-between align-items-center">
                                    <div class="d-flex flex-column">
                                        <span class="lead fw-normal">Track Order</span>
                                    </div>
                                </div>
                                <div class="container mt-3">
                                    <div class="row align-items-center">
                                        <div class="col-9">
                                            <h6>Address</h4>
                                                <div class="col-12">
                                                    <%=resolve[0].deliveryDetails.username%>
                                                </div>
                                                <div class="col-12">
                                                    <%=resolve[0].deliveryDetails.city%>,
                                                        <%=resolve[0].deliveryDetails.district%>,
                                                            <%=resolve[0].deliveryDetails.state%>
                                                </div>
                                                <div class="col-12">
                                                    <%=resolve[0].deliveryDetails.pincode%>
                                                </div>
                                                <div class="col-12 text-primary">
                                                    <%=resolve[0].deliveryDetails.email%>
                                                </div>
                                                <div class="col-12">Payment method : <span
                                                        class="text-success font-weight-bold">
                                                        <%=resolve[0].deliveryDetails.paymentMethod%>
                                                    </span></div>
                                                    <div class="col-12">
                                                        Payable amount : <span
                                                            class=" font-weight-bold">
                                                            â‚¹<%=resolve[0].result[0].price * resolve[0].products.quantity-resolve[0].products.applyCoupon%>
                                                        </span>
                                                    </div>
                                                <div class="col-12">
                                                    <%=resolve[0].date.toString().slice(0,15)%>
                                                </div>
                                        </div>
                                        <div class="col-3" style="max-width:150px ;">
                                            <div class="row justify-content-center">
                                                <img src="/img/product/<%= resolve[0].result[0].imageOne%>" alt="">
                                                <span>
                                                    <%= resolve[0].result[0].productName%>
                                                </span>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <hr class="my-4">
                                <% if (resolve[0].products.status=='cancelled' ) {%>
                                    <div class="col-12">
                                        <div class="progress" style="height: 6px; border-radius: 16px;">
                                            <div class="progress-bar" role="progressbar"
                                                style="width:<%=resolve[0].products.percentage%>%; border-radius: 10px; background-color:red;"
                                                aria-valuenow="65" aria-valuemin="0" aria-valuemax="100"></div>
                                        </div>
                                        <div class="d-flex justify-content-around mb-1">
                                            <p class="text-muted mt-1 mb-0 small ms-xl-5">Order Confirmed</p>
                                            <p class="text-muted mt-1 mb-0 small ms-xl-5">Shipped</p>
                                            <p class="text-muted mt-1 mb-0 small ms-xl-5">Out For Delivery</p>
                                            <p class="text-muted mt-1 mb-0 small ms-xl-5">Delivered</p>
                                        </div>
                                    </div>

                                    <div class="container mt-4">
                                        <div class="row">
                                            <div class="col d-flex justify-content-center">
                                                <h5 class="text-danger">Cancelled</h5>
                                            </div>
                                        </div>
                                    </div>

                                    <%}else if (resolve[0].products.status=='delivered' ){%>
                                        <div class="col-12">
                                            <div class="progress" style="height: 6px; border-radius: 16px;">
                                                <div class="progress-bar" role="progressbar"
                                                    style="width:<%=resolve[0].products.percentage%>%; border-radius: 10px; background-color: #0eb109;"
                                                    aria-valuenow="65" aria-valuemin="0" aria-valuemax="100"></div>
                                            </div>
                                            <div class="d-flex justify-content-around mb-1">
                                                <p class="text-muted mt-1 mb-0 small ms-xl-5">Order Confirmed</p>
                                                <p class="text-muted mt-1 mb-0 small ms-xl-5">Shipped</p>
                                                <p class="text-muted mt-1 mb-0 small ms-xl-5">Out For Delivery</p>
                                                <p class="text-muted mt-1 mb-0 small ms-xl-5">Delivered</p>
                                            </div>
                                        </div>

                                        <div class="container mt-4">
                                            <div class="row">
                                                <div class="col d-flex justify-content-center">
                                                    <a class="btn" style="text-transform: none !important"
                                                        href="">Return?</a>
                                                </div>
                                                <div class="col d-flex justify-content-center">
                                                    <a class="btn" style="text-transform: none !important" href="#">Give
                                                        feedback</a>
                                                </div>
                                            </div>
                                        </div>
                                        <%}else{%>
                                            <div class="col-12">
                                                <div class="progress" style="height: 6px; border-radius: 16px;">
                                                    <div class="progress-bar" role="progressbar"
                                                        style="width:<%=resolve[0].products.percentage%>%; border-radius: 10px; background-color: #0eb109;"
                                                        aria-valuenow="65" aria-valuemin="0" aria-valuemax="100"></div>
                                                </div>
                                                <div class="d-flex justify-content-around mb-1">
                                                    <p class="text-muted mt-1 mb-0 small ms-xl-5">Order Confirmed</p>
                                                    <p class="text-muted mt-1 mb-0 small ms-xl-5">Shipped</p>
                                                    <p class="text-muted mt-1 mb-0 small ms-xl-5">Out For Delivery</p>
                                                    <p class="text-muted mt-1 mb-0 small ms-xl-5">Delivered</p>
                                                </div>
                                            </div>

                                            <div class="container mt-4">
                                                <div class="row">
                                                    <div class="col d-flex justify-content-center">
                                                        <a class="btn" style="text-transform: none !important"
                                                            href="cancelOrder?productId=<%=resolve[0].products.productId%>&id=<%=resolve[0]._id%>">Cancel</a>
                                                    </div>
                                                    <div class="col d-flex justify-content-center">
                                                        <a class="btn" style="text-transform: none !important"
                                                            href="#">Need
                                                            help?</a>
                                                    </div>
                                                </div>
                                            </div>
                                            <%}%>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>




